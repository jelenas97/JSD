entity Prescription:
	properties:
		{Id}
		id > long

		{Column}
		valid > bool

		{ManyToMany}
		medications > [Medication]

	operations:
		updateValidity(id : long) bool
		addMedication(medication : Medication) bool
		removeMedication(id : long) bool

entity Medication:
	properties:
		{Id}
		id > long

		{Column}
		name > string

  operations:
    getById(id : long) Medication
		getAll() [Medication]
		save(medication : Medication) bool
		update(id : long, medication : Medication) bool
		delete(id : long) bool


shared[Medication, Prescription]:
  	properties:
		{Id}
		id > long

		{Column}
		name > string

	operations:
		getById
		getAll
		save
		update
		delete

pages[Medication, Prescription]:
		add
		view
		edit
