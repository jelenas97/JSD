entity Prescription:
	properties:
		{Column}
		title > string

		{Column}
		doctor > string

		{Column}
		valid > bool

		{ManyToMany}
		medications > [Medication]

	operations:
	    getById(id : long) Prescription
		getAll() [Prescription]
		updateValidity(id : long) bool
		addMedication(medication : Medication) bool
		removeMedication(id : long) bool

entity Medication:
	properties:
		{Column}
		code > string

		{Column}
		name > string

		{Column}
		type > string

		{Column}
		description > string

	operations:
		getById(id : long) Medication
		getAll() [Medication]


entity Patient:
	properties:
		{Column}
		firstname > string

		{Column}
		lastname > string

		{Column}
		city > string

		{Column}
		age > string

	operations:
		getById(id : long) Patient
		getAll() [Patient]

shared[Medication, Prescription, Patient]:
  properties:
		{Id}
		id > long

  operations:
    save
    update
    delete

pages[Medication, Prescription, Patient]:
  add
  view
  edit
