entity Prescription:
	properties:
		{Column}
		valid > bool

		{ManyToMany}
		medications > [Medication]

	operations:
		updateValidity(id : long) bool
		addMedication(medication : Medication) bool
		removeMedication(id : long) bool

entity Medication:
	properties:
		{Column}
		code > string

  operations:
    getById(id : long) Medication
		getAll() [Medication]

shared[Medication, Prescription]:
  properties:
		{Id}
		id > long

		{Column}
		name > string

  operations:
    save
    update
    delete

pages[Medication, Prescription]:
  add
  view
  edit
