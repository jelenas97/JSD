entity Prescription:
	properties:
		{Column}
		id > long

		{Column}
		valid > bool

		{ManyToMany}
		medications > [Medication]

	operations:
		updateValidity(id : long) bool
		addMedication(medication : Medication) bool
		removeMedication(id : long) bool

entity Medication:
	properties:
		{Column}
		id > long

		{Column}
		name > string


shared[Medication]:
  	properties:
		{Id}
		id > long

		{Column}
		name > string

	operations:
		getById
		getAll
		save
		update
		delete

pages[Medication, Prescription]:
		add
		view
		edit
